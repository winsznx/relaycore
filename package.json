{
  "name": "relaycore",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:graphql": "tsx watch src/api/graphql-server.ts",
    "dev:indexers": "tsx watch -r dotenv/config src/init.ts",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm dev:graphql\" \"pnpm dev:indexers\"",
    "build": "tsc -b && vite build",
    "build:graphql": "tsc --project tsconfig.graphql.json",
    "start": "vite preview",
    "start:graphql": "node dist/api/graphql-server.js",
    "start:indexers": "node dist/init.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "typecheck": "tsc --noEmit",
    "compile": "npx hardhat compile",
    "deploy:testnet": "npx hardhat run scripts/deploy.ts --network cronosZkevmTestnet",
    "deploy:mainnet": "npx hardhat run scripts/deploy.ts --network cronosZkevmMainnet",
    "verify:testnet": "npx hardhat verify --network cronosZkevmTestnet",
    "register-agent:testnet": "npx hardhat run scripts/register-agent.ts --network cronosZkevmTestnet"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.15.2",
    "@amcharts/amcharts5-geodata": "^5.1.5",
    "@anthropic-ai/sdk": "^0.71.2",
    "@apollo/server": "^5.2.0",
    "@crypto.com/facilitator-client": "^1.0.2",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@openzeppelin/contracts": "^5.4.0",
    "@pythnetwork/hermes-client": "^2.1.0",
    "@pythnetwork/pyth-evm-js": "^2.0.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-slot": "^1.2.4",
    "@reown/appkit": "^1.8.16",
    "@reown/appkit-adapter-wagmi": "^1.8.16",
    "@sentry/node": "^10.32.1",
    "@sentry/react": "^10.32.1",
    "@sentry/vite-plugin": "^4.6.1",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.17.19",
    "@telegraf/session": "2.0.0-beta.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node-schedule": "^2.1.8",
    "@vvs-finance/swap-sdk": "^0.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "express": "^5.2.1",
    "framer-motion": "^12.25.0",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "gsap": "^3.14.2",
    "ioredis": "^5.9.1",
    "lucide-react": "^0.562.0",
    "node-schedule": "^2.1.1",
    "passport": "^0.7.0",
